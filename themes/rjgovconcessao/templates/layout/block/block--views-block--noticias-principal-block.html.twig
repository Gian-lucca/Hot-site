{% if label %}
<section data-aos="zoom-in-up" class="programas-acoes branco aos-init aos-animate">
  <div class="section-container">    
    <div class="titulo-secao">
      <span>{{ label }}</span>
    </div>
  </div>
</section>
<section id="noticias">
  <div class="noticia-2col-wrap">
    {% block content %}
      {{ content }}
    {% endblock %}
  </div>
</section>
{% endif %}

